import random
import string
def letra_minuscula():
    return random.choice(string.ascii_lowercase)
def letra_mayuscula():
    return random.choice(string.ascii_uppercase)
def numero():
    return random.randint(1, 9)
def simbolo():
    return random.choice('*_@')
def simbolo():
    return random.choice('&/#')
contraseña = [
    str(random.randint(1, 5)),        
    letra_minuscula(),                
    letra_mayuscula(),              
    random.choice('*_@'),             
    letra_minuscula(),                
    str(random.randint(6, 9)),        
    random.choice('&/#'),             
    str(random.randint(0, 5))        
]
random.shuffle(contraseña)
contraseña_generada = ''.join(contraseña)
print("Contraseña generada:", contraseña_generada)
input()